package delivery.dao.api;

import java.util.List;

import delivery.api.mapper.EstadoImpl;
import delivery.api.mapper.UsuariosMobImpl;
import delivery.model.Estado;
import delivery.model.UsuarioMob;

public class Teste {

	public static void main(String[] args) {
		EstadoImpl estado = new EstadoImpl();
		List<Estado> estados = estado.getEstadosDAO();
		int counter = 0;
		for(Estado state: estados){
			counter++;
			System.out.println(counter + " " + state.getSigla());
		}
		
		
	}
}
