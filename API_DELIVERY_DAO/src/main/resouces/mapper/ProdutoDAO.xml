<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
 <mapper namespace="delivery.dao.ProdutoDAO">
 	<insert id="cadastrarProdutoDAO" parameterType="delivery.DELIVERY_MODELO.Produto">
 		insert into produto (id, decricao,preco, id_empresa) values ( #{id},#{descricao}, #{preco}, #{id_empresa} )
 	</insert>
 </mapper>

 